{
  "name": "mocoolka-fp",
  "version": "0.1.17",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "author": "fastspeeed@gmail.com",
  "scripts": {
    "reinstall": "rm -rf node_modules/ && yarn install",
    "clean": "rm -rf lib/ coverage/ ",
    "lint": "tslint -p './tsconfig.json'",
    "lint:fix": "tslint -p ./tsconfig.json --fix",
    "test": "jest",
    "test:watch": "jest --config jest.config.json --watch ./src",
    "build": "rm -rf lib/ && tsc ",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "prepublishOnly": "npm run clean && npm run reinstall && npm run lint && npm run build && npm run test",
    "coverage": "rimraf coverage && jest --config jest.config.json --coverage",
    "dtslint": "dtslint dtslint"
  },
  "dependencies": {
    "@types/lodash": "^4.14.116",
    "fp-ts": "^1.8.1",
    "io-ts": "^1.3.0",
    "io-ts-codegen": "^0.2.0",
    "io-ts-types": "^0.3.13",
    "lodash": "^4.17.11",
    "monocle-ts": "^1.2.0",
    "newtype-ts": "^0.2.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "dtslint": "^0.3.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(./src/*|\\.(test|spec))\\.(ts|tsx)$"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mocoolka/mocoolka-fp.git"
  },
  "keywords": [
    "mocoolka",
    "typescript",
    "algebraic-data-types",
    "functional-programming"
  ],
  "license": "MIT"
}
